@model IEnumerable<GarbageCollector.Models.PickupModels>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.User.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Street)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.User.Zip)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PickupDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.User.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Street)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.User.Zip)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PickupDate)
        </td>
    </tr>
}

</table>

